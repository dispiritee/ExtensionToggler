{
    "manifest_version": 3,
    "name": "Extension Toggler",
    "version": "0.1",
    "description": "Enable/disable added extensions.",
    "permissions": [
      "storage",
      "tabs",
      "management"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://www.youtube.com/*"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  