{
    "manifest_version": 3,
    "name": "Extension Toggler",
    "version": "0.3",
    "description": "Enable/disable added extensions.",
    "permissions": [
      "storage",
      "tabs",
      "management"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  